{
  // Comments are supported, like tsconfig.json
  "entryPoints": ["./src"],
  "entryPointStrategy": "expand",
  "out": "docs",
  "exclude": [
    "**/*.js",
    "**/*.json",
    "**/*.mjs",
    "**/*.cjs",
    "**/*.stories.*",
    "**/stories/**",
    "**/*.typegen.*",
    "**/*.d.ts",
    "/src/type-declarations"
  ],
  "plugin": ["typedoc-plugin-markdown", "typedoc-plugin-extras"]
}
